{
  "name": "discord-emoji",
  "description": "Unofficial but correct emoji index pulled directly from Discord with automatic updates for changes daily.",
  "version": "2.0.0",
  "license": "MIT",
  "author": "Samuel J Voeller <samuel.voeller@amethyst.live> (https://chat.amethyst.live/)",
  "contributors": [],
  "homepage": "https://github.com/amethyst-studio/discord-emoji",
  "repository": {
    "url": "git@github.com/amethyst-studio/discord-emoji.git"
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">6.14.0"
  },
  "engineStrict": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist/",
    "/package.json",
    "/README.md",
    "/LICENSE.md"
  ],
  "scripts": {
    "prepare": "husky install",
    "prepack": "npm run build",
    "stage": "eslint .",
    "build": "(npm run stage) && (npx tsc --build) && (mocha './tests/**/*.js')",
    "docs": "typedoc --out ./docs/ ./lib/",
    "upgrade": "npx npm-check-updates -u --target latest --peer && wget https://www.toptal.com/developers/gitignore/api/node,yarn,webstorm+all,visualstudiocode,visualstudio -O ./.gitignore && cat ./custom.gitignore >> .gitignore"
  },
  "devDependencies": {
    "@types/chai": "^4.2.16",
    "@types/js-beautify": "^1.13.1",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "chai": "^4.3.4",
    "del": "^6.0.0",
    "eslint": "^7.26.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-config-standard-with-typescript": "^20.0.0",
    "eslint-import-resolver-node": "^0.3.4",
    "eslint-plugin-import": "^2.23.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-standard": "^5.0.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "eslint-plugin-typescript-sort-keys": "^1.6.0",
    "husky": "^6.0.0",
    "js-beautify": "^1.13.13",
    "mocha": "^8.4.0",
    "node-fetch": "^3.0.0-beta.9",
    "typedoc": "^0.20.36",
    "typescript": "^4.2.4",
    "website-scraper": "^4.2.3"
  },
  "root": true
}
